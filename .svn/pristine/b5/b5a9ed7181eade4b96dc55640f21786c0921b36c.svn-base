package model;

import java.util.ArrayList;

public class Storage extends InfomationalResource {
	
	 private ArrayList<InfPackage> packages;
	 
	
	 public Storage() {
		 this.packages = new ArrayList<>();
	}
	 
	public void addPackage(InfPackage infPackage){
		packages.add(infPackage);
		add(infPackage);
	}
	public ArrayList<InfPackage> getPackages() {
		return packages;
	}
	 
}
